            "repeatLines": [
                [
                    "Der Fisch stinkt vom Kopf her."
                ]
            ]
        },
        {
            "eventName": "ProspectorBoss",
            "mainLines": [
                "Mein gott! The noise!"
            ],
            "repeatLines": [
                [
                    "...be swift, and strike true."
                ]
            ]
        },
        {
            "eventName": "TrapperTraderBoss",
            "mainLines": [
                "[e:anger]I shall not fall to you!"
            ],
            "repeatLines": [
                [
                    "The Schrieber hides",
                    "his face behind a mask...",
                    "[e:anger]Pathetic."
                ]
            ]
